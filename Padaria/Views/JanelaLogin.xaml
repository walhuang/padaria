<Window x:Class="Padaria.Views.JanelaLogin"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Padaria.Views"
        mc:Ignorable="d"
        Title="JanelaLogin" Height="450" Width="800">
    <Grid>
        <Label Content="labelLogin" HorizontalAlignment="Left" Margin="152,117,0,0" VerticalAlignment="Top"/>
        <Label Content="labelSenha" HorizontalAlignment="Left" Margin="152,200,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="tbLogin" HorizontalAlignment="Left" Height="23" Margin="255,117,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="120" RenderTransformOrigin="0.492,-0.609"/>
        <TextBox x:Name="tbSenha" HorizontalAlignment="Left" Height="23" Margin="255,200,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="120"/>
        <Button x:Name="btnEntrar" Content="Button" HorizontalAlignment="Left" Margin="283,312,0,0" VerticalAlignment="Top" Width="75" Click="btnEntrarClick"/>

    </Grid>
</Window>
