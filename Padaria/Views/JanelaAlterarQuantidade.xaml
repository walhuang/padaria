<Window x:Class="Padaria.Views.JanelaAlterarQuantidade"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Padaria.Views"
        mc:Ignorable="d"
        Title="JanelaAlterarQuantidade" Height="450" Width="800">
    <Grid>
        <Label Content="Insira nova quantidade" HorizontalAlignment="Left" Margin="167,114,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="txQuantidade" HorizontalAlignment="Left" Height="23" Margin="167,238,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="120"/>
        <Button x:Name="btnQuantidade" Content="Button" HorizontalAlignment="Left" Margin="349,238,0,0" VerticalAlignment="Top" Width="75" Click="btnQuantidadeClick"/>

    </Grid>
</Window>
